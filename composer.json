{
    "name": "pihedy/rc-network-php-client",
    "description": "The client that controls the RC devices.",
    "type": "project",
    "autoload": {
        "files": [
            "src/app/helper.php"
        ],
        "psr-4": {
            "RcNetwork\\": "src/app/"
        }
    },
    "config": {
        "vendor-dir": "src/vendor"
    },
    "require": {
        "php": ">=8.1.0",
        "react/datagram": "^1.9",
        "psr/container": "^2.0",
        "symfony/console": "^7.0",
        "cboden/ratchet": "^0.5.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
